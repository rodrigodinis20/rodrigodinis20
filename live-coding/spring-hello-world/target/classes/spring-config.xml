<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="hello" class="org.academiadecodigo.cachealots.HelloWorld">
        <property name="name" value="Don Ruan Carte D'or Ferrero Rocher"/>
    </bean>

    <bean id="constructor" class="org.academiadecodigo.cachealots.HelloWorld">
        <constructor-arg name="name" value="Rui"/>
    </bean>

</beans>